# MQTT broker related config
[mqtt]
host = 127.0.0.1
port = 1883
login = mqttlogin # is optional
password = mqttpassword # is optional, but needed if login is set

# root of the used topics
roottopic = vlx2mqtt
# topic for status messages
statustopic = status

# KLF200 related config
[velux]
host = VELUX_KLF_3CB3
password = klf200WiFiPassword

[log]
verbose = false
logfile = /tmp/vlx2mqtt.log